import java.util.*;
class append
{
public static void main(String args[])
{
ArrayList<Integer>a=new ArrayList<Integer>();
ArrayList<Integer>a1=new ArrayList<Integer>();
a1.add(20);
a1.add(10);
a1.add(40);
a1.add(30);
a.add(400);
a.add(200);
a.add(100);
a.add(400);
System.out.println("First array "+a);
System.out.println("Second array "+a1);
a1.addAll(a);
System.out.println("final list "+a1);
Collections.sort(a1);
System.out.println("Sorted array"+a1);
}
}
